<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.1.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />


	<jaxws:endpoint id="ssWebService" address="/ssWebService"
		implementor="com.sshome.ssmcxf.webservice.impl.SsWebServiceImpl" />

	<jaxws:endpoint id="weldingMachineWebService" address="/weldingMachineWebService"
		implementor="com.sshome.ssmcxf.webservice.impl.WeldingMachineWebServiceImpl" />

	<jaxws:endpoint id="insfWebService" address="/insfWebService"
		implementor="com.sshome.ssmcxf.webservice.impl.insfWebServiceImpl" />
		
	<jaxws:endpoint id="userWebService" address="/userWebService"
		implementor="com.sshome.ssmcxf.webservice.impl.UserWebServiceImpl" />
		
	<jaxws:endpoint id="liveDataWebService" address="/liveDataWebService"
		implementor="com.sshome.ssmcxf.webservice.impl.LiveDataWebServiceImpl" />
</beans>
